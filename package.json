{
  "name": "@openquantum/honey-pager",
  "version": "0.2.0",
  "description": "A Relay-style cursor pagination tool for mongoose schema.",
  "main": "build/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint index.js lib/* graphql/*",
    "test": "yarn run lint && jest",
    "clean": "rm -rf build",
    "prepare": "npm run build",
    "build": "yarn run clean && babel -d ./build --ignore node_modules,*test ."
  },
  "repository": "git@github.com:codeinbrain/honey-pager.git",
  "author": "Louis Bodart <lo.bodart3@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "build",
    "LICENSE",
    "README.MD"
  ],
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "mongoose": "^5.13.2"
  },
  "dependencies": {
    "base64-url": "^2.3.3",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "validator": "^13.6.0"
  }
}
